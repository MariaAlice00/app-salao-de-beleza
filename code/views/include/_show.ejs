<% for(var i = 0; i < services.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= services[i].type %></td>
        <td><%= services[i].duration %></td>
        <td><%= services[i].products %></td>
        <td><%= services[i].price %></td>
        <td>
            <a href="/update-service?id=<%= services[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= services[i]._id%> >
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
<% } %>